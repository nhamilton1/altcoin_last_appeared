// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model coins {
  coin_name      String    @id @unique
  price          Json[]
  first_appeared DateTime
  last_appeared  DateTime?
}

model coinsTest {
  coin_name      String    @id @unique
  first_appeared DateTime
  last_appeared  DateTime?
  price_date     DateTime[]
  price          Float[]
}

model coinsTest2 {
  coin_name      String    @id @unique
  price          String[]
  first_appeared DateTime
  last_appeared  DateTime?
}

model coinsTest3 {
  coin_name      String    @id @unique
  price          Json[]
  first_appeared DateTime
  last_appeared  DateTime?
}